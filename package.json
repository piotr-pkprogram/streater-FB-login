    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-modal": "^3.14.4",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0"
